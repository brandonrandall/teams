<h1>Log In</h1>

<%= form_for :session, url: login_path do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :confirmation_password %>
  <%= f.password_field :confirmation_password %>

  <%= f.submit "Log In" %>
<% end %>
