<%= form_for(@coach) do |f| %>
  <% if @coach.errors.any? %>
    <h2><%= pluralize(@coach.errors.count, "error") %>
            prohibited this record from being saved:</h2>
    <ul>
      <% @coach.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
    </ul>
  <% end %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :ipassword %>
  <%= f.password_field :password %>

  <%= f.submit %>
<% end %>
